{
  "name": "pgf-asesoramiento-docente-api",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "lint": "eslint . --ext .ts",
    "test": "jest --passWithNoTests",
    "test:e2e": "jest --config ./test/jest-e2e.json --passWithNoTests",
    "typeorm": "typeorm-ts-node-commonjs -d src/data-source.ts",
    "typeorm:migrate": "npm run typeorm -- migration:generate src/migrations/Init",
    "typeorm:run": "npm run typeorm -- migration:run",
    "typeorm:revert": "npm run typeorm -- migration:revert",
    "seed": "ts-node src/seeds/seed.ts",
    "dev": "npm run start:dev"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "11.1.6",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "11.1.6",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.1.6",
    "@nestjs/swagger": "11.2.1",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "11.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "glob": "^10.4.5",
    "multer": "^2.0.2",
    "openid-client": "^5.6.5",
    "papaparse": "^5.5.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "0.1.14",
    "rimraf": "^4.4.1",
    "rxjs": "7.8.2",
    "superagent": "^10.2.3",
    "swagger-ui-express": "5.0.1",
    "typeorm": "^0.3.20",
    "uuid": "^13.0.0",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.2",
    "@nestjs/schematics": "11.0.1",
    "@nestjs/testing": "11.1.6",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^4.17.24",
    "@types/jest": "^29.5.12",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.5.1",
    "@types/papaparse": "^5.3.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  }
}
